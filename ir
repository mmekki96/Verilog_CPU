    .scope S_0x5c61495842d0;
T_0 ;
    %wait E_0x5c61495824e0;
    %load/vec4 v0x5c6149573ad0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5c6149574420_0;
    %assign/vec4 v0x5c614959e760_0, 0;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c61495842d0;
T_1 ;
    %wait E_0x5c6149549f50;
    %load/vec4 v0x5c614959e760_0;
    %parti/s 4, 4, 4;
    %assign/vec4 v0x5c614959e550_0, 0;
    %load/vec4 v0x5c614959e760_0;
    %parti/s 4, 0, 2;
    %assign/vec4 v0x5c614959e630_0, 0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5c6149584140;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c614959e8e0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5c614959e8e0_0;
    %inv;
    %store/vec4 v0x5c614959e8e0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5c6149584140;
T_3 ;
