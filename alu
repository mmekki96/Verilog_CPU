#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x631efa105450 .scope module, "tb_alu" "tb_alu" 2 3;
 .timescale -9 -12;
v0x631efa120160_0 .var "in1_tb", 3 0;
v0x631efa120240_0 .var "in2_tb", 3 0;
v0x631efa120310_0 .net "out_tb", 3 0, v0x631efa11fee0_0;  1 drivers
v0x631efa120410_0 .var "sel_tb", 2 0;
S_0x631efa1055e0 .scope module, "UUT" "alu" 2 10, 3 1 0, S_0x631efa105450;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in1";
    .port_info 1 /INPUT 4 "in2";
    .port_info 2 /INPUT 3 "sel";
    .port_info 3 /OUTPUT 4 "out";
v0x631efa0ca490_0 .net "in1", 3 0, v0x631efa120160_0;  1 drivers
v0x631efa11fe00_0 .net "in2", 3 0, v0x631efa120240_0;  1 drivers
v0x631efa11fee0_0 .var "out", 3 0;
v0x631efa11ffd0_0 .net "sel", 2 0, v0x631efa120410_0;  1 drivers
E_0x631efa0caf20 .event anyedge, v0x631efa11ffd0_0, v0x631efa0ca490_0, v0x631efa11fe00_0;
    .scope S_0x631efa1055e0;
T_0 ;
    %wait E_0x631efa0caf20;
    %load/vec4 v0x631efa11ffd0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %pushi/vec4 15, 15, 4;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.0 ;
    %load/vec4 v0x631efa0ca490_0;
    %load/vec4 v0x631efa11fe00_0;
    %add;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.1 ;
    %load/vec4 v0x631efa0ca490_0;
    %load/vec4 v0x631efa11fe00_0;
    %sub;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.2 ;
    %load/vec4 v0x631efa0ca490_0;
    %load/vec4 v0x631efa11fe00_0;
    %and;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.3 ;
    %load/vec4 v0x631efa0ca490_0;
    %load/vec4 v0x631efa11fe00_0;
    %or;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.4 ;
    %load/vec4 v0x631efa0ca490_0;
    %load/vec4 v0x631efa11fe00_0;
    %xor;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.5 ;
    %load/vec4 v0x631efa0ca490_0;
    %inv;
    %store/vec4 v0x631efa11fee0_0, 0, 4;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x631efa105450;
T_1 ;
    %vpi_call 2 18 "$dumpfile", "alu.vcd" {0 0 0};
    %vpi_call 2 19 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x631efa105450 {0 0 0};
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 23 "$display", "Time=%0t: Add %b + %b = %b", $time, v0x631efa120160_0, v0x631efa120240_0, v0x631efa120310_0 {0 0 0};
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 25 "$display", "Time=%0t: Substract %b - %b = %b", $time, v0x631efa120160_0, v0x631efa120240_0, v0x631efa120310_0 {0 0 0};
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 27 "$display", "Time=%0t: And gate %b And %b = %b", $time, v0x631efa120160_0, v0x631efa120240_0, v0x631efa120310_0 {0 0 0};
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 29 "$display", "Time=%0t: Or gate %b Or %b = %b", $time, v0x631efa120160_0, v0x631efa120240_0, v0x631efa120310_0 {0 0 0};
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 31 "$display", "Time=%0t: Xor gate %b Xor %b = %b", $time, v0x631efa120160_0, v0x631efa120240_0, v0x631efa120310_0 {0 0 0};
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x631efa120160_0, 0, 4;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x631efa120240_0, 0, 4;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x631efa120410_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 33 "$display", "Time=%0t: Not gate of %b = %b", $time, v0x631efa120160_0, v0x631efa120310_0 {0 0 0};
    %delay 10000, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
